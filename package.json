{
  "name": "vscode-dependency-graph",
  "displayName": "Dependency Graph",
  "description": "Visualize OOP service dependencies (NestJS, Inversify, Awilix) as a graph.",
  "author": {
    "name": "Jo√£o Luiz Pereira Ferreira",
    "email": "joaoferreira981011@gmail.com"
  },
  "icon": "assets/logo.jpg",
  "version": "0.0.1",
  "publisher": "joaoferreira981011",
  "main": "./dist/extension.js",
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.96.0"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.showDependencyGraph",
        "title": "Show Dependency Graph"
      }
    ],
    "activationEvents": [
      "onCommand:extension.showDependencyGraph"
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.0.0",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "glob": "^11.0.2",
    "mocha": "^11.6.0",
    "npm-run-all": "^4.1.5",
    "sinon": "^20.0.0",
    "ts-morph": "17.0.1",
    "typescript": "4.9.5"
  }
}
